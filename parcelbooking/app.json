{
  "expo": {
    "name": "parcelbooking",
    "slug": "parcelbooking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/parcelwalla.png",
    "scheme": "parcelbooking",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ratlam.parcelbooking"
    },
    "android": {
      "package": "com.ratlam.parcelbooking",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#FF6B35",
        "foregroundImage": "./assets/images/parcelwalla.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "notification": {
        "icon": "./assets/images/parcelwalla.png",
        "color": "#FF6B35"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "parcelbooking",
              "host": "payment"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/parcelwalla.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/parcelwalla.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#1a1a1a"
          }
        }
      ],
      "react-native-reanimated/plugin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d6c7139a-de05-4408-8edf-5db1e53b983f"
      },
      "onesignal": {
        "appId": "0f28bb2e-e63e-4bfa-9710-77dcaf7b3aa7"
      }
    }
  }
}
